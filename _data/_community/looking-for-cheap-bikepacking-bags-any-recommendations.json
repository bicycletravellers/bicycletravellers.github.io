ntBlockTune":{"alignment":"left"}}},{"id":"id_912594","type":"fbcomment","data":{"text":"<div class=\"comment_text\"><span class=\"ce-fbcomment-quote\" data-quote=\"Jacob Wood\"> cool let me know you got any questions on any of them. üëç</span></div>","author":"Jon Manning","gender":"MALE","level":"2"},"tunes":{"AlignmentBlockTune":{"alignment":"left"}}},{"id":"id_634515","type":"fbcomment","data":{"text":"<div class=\"comment_text\">Planet x for bar, fork and saddle bags, ebay or amazon for frame bag.</div><div class=\"comment_attachment\"><div class=\"comment_photo_attachment\"><img class=\"comment_photo_image\" loading=\"lazy\" src=\"/images/community/looking-for-cheap-bikepacking-bags-any-recommendations/image_1655565286_2168493679.jpg\" alt=\"\"></div></div>","author":"Emma Downey","gender":"FEMALE","level":"0"},"tunes":{"AlignmentBlockTune":{"alignment":"left"}}},{"id":"id_796055","type":"fbcomment","data":{"text":"<div class=\"comment_text\">Check out the Bearbones forum classified advert section.</div>","author":"Dave Robinson","gender":"MALE","level":"0"},"tunes":{"AlignmentBlockTune":{"alignment":"left"}}},{"id":"id_43507","type":"fbcomment","data":{"text":"<div class=\"comment_text\">Selling my Miss Grape Internode frame bag.....not for very cheap though üòÑ</div>","author":"Valerio Stuart","gender":"MALE","level":"0"},"tunes":{"AlignmentBlockTune":{"alignment":"left"}}}]}